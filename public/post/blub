UNIX_TIMESTAMP(DATE_SUB(curdate(),INTERVAL 2 MONTH));


select * from prosodyarchive where 'when' < UNIX_TIMESTAMP(DATE_SUB(curdate(),INTERVAL 2 MONTH));
